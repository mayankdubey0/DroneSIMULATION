
classdef Drone_Controller < handle
    properties
        drone
        target
        error
        proportional
        integral
        derivative
        system_function
        transfer_function
        symbols, angles
        motor_speeds
        accel_reading, gyro_reading
        thrusts

    end
    
    methods
        function obj = Drone_Controller()
            obj.drone = Drone();
            obj.target = 0;
            obj.error = 0;
            obj.proportional = 0;
            obj.integral = 0;
            obj.derivative = 0;
            obj.motor_speeds  = [1100; 1050; -900; -900];

            obj.thrusts = thrust_cons * air_density * (diameter^4) * [obj.motor_speeds(1)^2; obj.motor_speeds(2)^2; obj.motor_speeds(3)^2; obj.motor_speeds(4)^2];
            

            obj.symbols = sym('t s');
            obj.system_function = [(drone.gravity + sum(obj.thrusts))* drone.total_mass = ];
            

        end
    end

end
