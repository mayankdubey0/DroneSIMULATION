
classdef Drone_Controller < handle
    properties
        drone
        target
        error
        proportional
        integral
        derivative
        system_function
        transfer_function
        symbols, angle
        motor_speeds
        accel_reading, gyro_reading

    end
    
    methods
        function obj = Drone_Controller()
            obj.drone = Drone();
            obj.target = 0;
            obj.error = 0;
            obj.proportional = 0;
            obj.integral = 0;
            obj.derivative = 0;
            obj.motor_speeds  = [1100; 1050; -900; -900];

            obj.symbols = sym('t s');
            obj.angle = motor_speeds*t;
            obj.system_funtion = [drone.Ixc * ]
            

        end
    end

end

